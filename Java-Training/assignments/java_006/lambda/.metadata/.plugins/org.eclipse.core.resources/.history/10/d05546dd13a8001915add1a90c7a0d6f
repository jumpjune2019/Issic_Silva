package headortail;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		RandomNumber random = () -> {
			int max = 2;
			int min = 1;
			
			int range = max-min;
			
			int rand = (int)(Math.random() * range) + min;
			
			return rand;
		};
		
		System.out.println(random.onetwo());
	}

}
